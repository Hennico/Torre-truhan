            <string>/// test laberinto
if (test) {
    randomize();
    var seed = random_get_seed();
    with (obj_pared) { instance_destroy(); }
    with (obj_flood) { instance_destroy(); }
    with (obj_personaje) { instance_destroy(); }
    
    var lab_ancho = 15;
    var lab_alto  = 15;
    
    var spawnX = 1;
    var spawnY = 1;
    
    var seed = random_get_seed();
    for (var polish = 0;polish &lt; 4; polish++) {
        random_set_seed(seed);
        //random_set_seed(2878124165);
        //random_set_seed(3889816275);
        
        var laberinto = ds_grid_create(lab_ancho, lab_alto);
        ds_grid_mazeify(laberinto, true, false, spawnX,spawnY, polish)
        //var laberinto = ds_grid_maze(lab_ancho,lab_alto,false, true, 5, 5, MazeOrientation.north_to_east,polish);
        
        var xoffset = 16+(polish mod 2)*512;
        var yoffset = 16+(polish div 2)*512;
        var celda_ancho = 32;
        var celda_alto  = 32;
        
        instance_create(xoffset+celda_ancho*spawnX+16, yoffset+celda_alto*spawnY+16, obj_personaje);
        instance_create(xoffset+celda_ancho*spawnX+16, yoffset+celda_alto*spawnY+16, obj_flood);
        
        for (var xx = 0; xx &lt; lab_ancho; xx ++) {
        for (var yy = 0; yy &lt; lab_alto ; yy ++) {
            var relX = xoffset+celda_ancho*xx;
            var relY = yoffset+celda_alto *yy;
            
            if (laberinto[# xx,yy])
            instance_create(relX,relY,obj_pared)
        }}
        ds_grid_destroy(laberinto);
    }
    
    //scr_agregarEfectoParedes(background3, -1)
}
