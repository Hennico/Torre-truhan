var hori_offset = 8;
draw_set_halign(fa_center)
draw_text(x+anchoBarra/2, y+vert_offset, "/");
draw_set_halign(fa_right)
draw_text(x+anchoBarra/2-hori_offset, y+vert_offset, string(seleccion.salud));
draw_set_halign(fa_left)
draw_text(x+anchoBarra/2+hori_offset, y+vert_offset, string(seleccion.salud));

</string>
