          <argument>
            <kind>1</kind>
            <string>/// Dibujar niebla
if (global.sigTurno) {
    surface_free(imagen_niebla);
}

if (!surface_exists(imagen_niebla)) {
    var pjPrevX = xToCell(obj_personaje.xprevious);
    var pjPrevY = yToCell(obj_personaje.yprevious);
    
    var pjX = xToCell(obj_personaje.x);
    var pjY = yToCell(obj_personaje.y);
    
    grilla_luz[# pjPrevX, pjPrevY] = 0;
    grilla_luz[# pjX, pjY] = 5;
    scr_iluminar(grilla_niebla, grilla_luz);

    imagen_niebla = surface_create(ESENARIO_ANCHO, ESENARIO_ALTO);
    
    surface_set_target(imagen_niebla);
    draw_set_alpha(1);
    draw_clear(c_black);
    draw_set_colour(c_black);
    draw_set_blend_mode(bm_subtract);
    for (var xx = 0; xx &lt; PLANO_ANCHO; xx++) {
    for (var yy = 0; yy &lt; PLANO_ALTO ; yy++) {
        var relX = cellToX(xx);
        var relY = cellToY(yy);
        var sigX = cellToX(xx+1);
        var sigY = cellToY(yy+1);
        
        var luz = grilla_niebla[# xx,yy];
        if (luz &gt; 0) {
            draw_set_alpha(luz);
            draw_rectangle(relX, relY, sigX-1, sigY-1, false);
        }
    }}
    draw_set_alpha(1);
    draw_set_blend_mode(bm_normal);
    surface_reset_target();
}

draw_surface(imagen_niebla,0,0);

</string>
